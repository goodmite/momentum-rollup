{
  "name": "imi-chat-preview",
  "version": "0.0.23",
  "scripts": {
    "start": "node lang.js && browser-sync public --no-ghost-mode --port 3001",
    "env:sharable": "node env-script.js __IS_EMBED_MODE=false",
    "env:sharable:prod": "node env-script.js __IS_EMBED_MODE=false __prod=true",
    "env:embed": "node env-script.js __IS_EMBED_MODE=true __MODE=embed_mode __deployRoot=http://localhost:5000",
    "env:embed:prod": "node env-script.js __IS_EMBED_MODE=true __MODE=embed_mode __deployRoot=https://imi-preview2.netlify.app __prod=true",
    "build": "rollup -c",
    "build:prod": "npm run env:sharable:prod && npm run build",
    "build:embed:prod": "npm run env:embed:prod && npm run build",
    "dev": "rollup -c -w",
    "embed:dev": "npm run env:embed && rollup -c -w",
    "embed:prod": "npm run env:embed:prod && rollup -c",
    "deploy:beta": "node deploy.js && git push",
    "deploy": "npm run build:prod && git add . && git commit && git push",
    "release-local": "npm run build:prod && node copy-to-imi.js",
    "release-embed": "npm run build && node copy-to-imi.js",
    "release": "node change-lib-name.js && npm run build && npm publish --access=public"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "browser-sync": "^2.26.13",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "git-last-commit": "^1.0.0",
    "marked": "^1.2.4",
    "ncp": "^2.0.0",
    "node-sass": "^4.14.1",
    "rollup": "^2.3.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.0.10"
  },
  "dependencies": {
    "@momentum-ui/core": "19.9.12",
    "@momentum-ui/icons": "7.54.0",
    "@momentum-ui/tokens": "1.5.0",
    "@momentum-ui/utils": "^6.2.7",
    "@momentum-ui/web-components": "2.0.14",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-json": "^4.1.0",
    "async": "^3.2.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "npm": "^6.14.11",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-scss": "^2.6.1",
    "simple-git": "^2.31.0",
    "svelte-i18n": "^3.3.0",
    "svelte-infinite-scroll": "^1.5.2",
    "lit-element": "2.3.1"
  },
  "main": "public/build/bundle.js"
}
